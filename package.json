{
  "name": "phoenixeye",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "scripts": {
    "postinstall": "node ./node_modules/bower/bin/bower install && node ./node_modules/gulp/bin/gulp.js",
    "test-server": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha --dir ./test/coverage/server -- --recursive ./test/src/server",
    "test-client": "node ./node_modules/karma/bin/karma start",
    "test": "npm run-script test-server && npm run-script test-client",
    "posttest": "rm -f tmp/*",
    "test-ci": "npm run-script test-server && npm run-script test-client",
    "posttest-ci": "node node_modules/lcov-result-merger/bin/lcov-result-merger.js './test/coverage/**/lcov.info' | node ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "ebemunk <ebemunk@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^1.0.0",
    "async": "^1.2.1",
    "body-parser": "^1.13.0",
    "busboy": "~0.2.9",
    "compression": "^1.5.0",
    "dataobject-parser": "~1.0.6",
    "debug": "^2.2.0",
    "express": "^4.12.4",
    "extend": "^2.0.1",
    "gm": "~1.18.1",
    "image-size": "~0.3.5",
    "md5-file": "~2.0.0",
    "mongoose": "^4.0.5",
    "monq": "~0.3.1",
    "request": "^2.57.0",
    "shortid": "^2.2.2"
  },
  "devDependencies": {
    "bower": "^1.4.1",
    "browser-sync": "^2.7.10",
    "chai": "~3.0.0",
    "coveralls": "^2.11.2",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-concat": "^2.5.2",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.1.6",
    "gulp-plumber": "^1.0.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.2.0",
    "istanbul": "^0.3.15",
    "karma": "^0.12.36",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.1.12",
    "karma-coverage": "^0.4.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.1.10",
    "karma-sinon": "^1.0.4",
    "lcov-result-merger": "^1.0.2",
    "mocha": "~2.2.5",
    "mockgoose": "~2.0.3",
    "rewire": "~2.3.4",
    "sinon": "~1.15.3",
    "supertest": "~1.0.1"
  }
}
